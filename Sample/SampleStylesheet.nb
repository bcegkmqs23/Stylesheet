\>", "Text",ExpressionUUID->"e789c842-94d9-4e70-a4cc-47c2253701c7"],

Cell[StyleData["Footnote", StyleDefinitions -> StyleData["StandardText"]],
 Evaluatable->True,
 CellEvaluationFunction->(Apply[
   Function[{fnlabel, fntext}, SelectionMove[
      EvaluationCell[], After, Cell]; SelectionMove[
      PreviousCell[CellStyle -> "FootnoteDocker"], After, Cell]; NotebookWrite[
      EvaluationNotebook[], 
      Cell[
       BoxData[
        RowBox[{
          Cell[
           BoxData[
            ButtonBox[
             Cell[
              TagBox[
               RowBox[{"[", 
                 CounterBox["FootnoteText"], "]"}], 
               BoxID -> {"FootnoteNum", fnlabel}], "Link"], ButtonFunction :> 
             NotebookLocate[
               StringJoin["FootnoteLabel", fnlabel]], Appearance -> 
             "Frameless", Evaluator -> Automatic, Method -> "Preemptive"]]], 
          fntext}]], "FootnoteText", CellTags -> {
         StringJoin["FootnoteText", fnlabel]}]]; NotebookWrite[
      EvaluationCell[], 
      Cell[
       BoxData[
        ButtonBox[
         Cell[
          BoxData[
           DynamicBox[
            Refresh[
             Cell[
              BoxData[
               FrontEndExecute[
                FrontEnd`BoxReferenceRead[
                 FE`BoxReference[
                  EvaluationNotebook[], {{"FootnoteNum", fnlabel}}, 
                  FE`BoxOffset -> {
                    FE`BoxChild[1]}, FE`SearchStart -> 
                  "StartFromBeginning"]]]]], UpdateInterval -> 1]]], "Link"], 
         ButtonFunction :> NotebookLocate[
           StringJoin["FootnoteText", fnlabel]], Appearance -> "Frameless", 
         Evaluator -> Automatic, Method -> "Preemptive"]], "FootnoteNum", 
       CellTags -> {
         StringJoin["FootnoteLabel", fnlabel]}]]], 
   ToExpression[#]]& ),
 CellFrameLabels->{{
